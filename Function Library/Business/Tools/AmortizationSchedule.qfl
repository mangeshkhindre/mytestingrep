	intPeriod3						=	CDbl (GUI_Object_GetPropertyValue ( objLoan.WebEdit("html id:=TextBox82"), "Value" ))
	intPrincipalAndInterest3		=	CDbl (GUI_Object_GetPropertyValue ( objLoan.WebEdit("html id:=pjtMaxBox31"), "Value" ))
